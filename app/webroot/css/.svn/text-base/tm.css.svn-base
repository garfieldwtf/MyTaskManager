/*for confirm box*/
.question {
	position: absolute;
	display: inline;
	text-align: center;
	width: 178px;
	height: 90px;
	font-size: 13px;
	line-height: 1.5em;
	background: url('../img/bubble.png') left top no-repeat;
	padding: 10px 100px 0px 10px;
	text-shadow: 0px 1px 0px #fff;
	margin-left: -8.5em;
	margin-top: -6.5em;
	opacity: 1;
}

.questionright {
	position: absolute;
	display: inline;
	text-align: center;
	width: 178px;
	height: 90px;
	font-size: 13px;
	line-height: 1.5em;
	background: url('../img/bubble.png') left top no-repeat;
	padding: 10px 100px 0px 10px;
	text-shadow: 0px 1px 0px #fff;
	margin-left: 75%;
	margin-top: -6.5em;
    float:right;
	opacity: 1;
}

.yes, .cancel {
    display: inline-block;
    text-decoration: none;
    width: auto;
    vertical-align: bottom;
    border-right: 2px outset black;
    border-bottom: 2px outset black;
    border-top: 1px outset black;
    border-left: 1px outset black;
    font-size: 12;
    padding: 3px 5px;
    color:#204b85; 
    background: url(../img/chromebg2.gif) center center repeat-x;
    margin-top:3px;
    -moz-border-radius: 5px;
	-webkit-border-radius: 5px;
    margin-right: .5em;
    text-shadow: 0px 1px 0px #000;
    cursor: pointer;
}

/***prompt, alert box design *******/
#popup_container {
	font-family: Arial, sans-serif;
	font-size: 12px;
	min-width: 300px; /* Dialog will be no smaller than this */
	max-width: 600px; /* Dialog will wrap after this width */
	background: #FFF;
	border: solid 5px #02A3F4;
	color: #000;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
    position:absolute;
}

.ui-dialog{
	font-family: Arial, sans-serif;
	font-size: 12px;
	min-width: 500px; /* Dialog will be no smaller than this */
	max-width: 1000px; /* Dialog will wrap after this width */
	background: #FFF;
	border: solid 5px #02A3F4;
	color: #000;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
    position:absolute;
}

.ui-dialog-titlebar,#popup_title {
	font-size: 14px;
	font-weight: bold;
	text-align: center;
	line-height: 1.75em;
	color: #666;
	background-color: #A5E9F2;
	border-bottom: solid 1px #999;
	cursor: default;
	padding: 0em;
	margin: 0em;
}

.ui-dialog-titlebar-close {
   display: none;
}

.ui-dialog-content,#popup_content {
	background: #FFFFFF 16px 16px;
	padding: 1em 1.75em;
	margin: 0em;
    bottom: 1em;
}

#popup_message {
	padding-left: 48px;
}

#popup_panel {
	text-align: center;
	margin: 1em 0em 0em 1em;
}

#popup_prompt {
	margin: .5em 0em;
}

#boxes .window {
  position:absolute;
  left:0;
  top:0;
  width:300px;
  display:none;
  z-index:9999;
  font-family: Arial, sans-serif;
    font-size: 12px;
	background: #FFF;
	border: solid 5px #02A3F4;
	color: #000;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
}

.ui-widget-overlay{
    position:absolute;
    left:0;
    top:0;
    z-index:9000;
    background-color:#000;
    opacity:0.8;
}

#mask {
  position:absolute;
  left:0;
  top:0;
  z-index:9000;
  background-color:#000;
  display:none;
}



/****grade and job scheme**********/
/* root element for tabs  */
ul.tabs {  
	margin:0 !important; 
	left:20px;
	height:30px;
	border-bottom:1px solid #666;	
    position:relative;
    width:97.5%;
    margin-top:0px;
}

/* link inside the tab.*/
ul.tabs a { 
	float:left;
	font-size:13px;
	display:block;
	padding:5px 30px;	
	text-decoration:none;
	border:2px solid black;	
	border-bottom:0px;
	height:18px;
	background-color:#efefef;
	color:#777;
	margin-right:2px;
	position:relative;
	top:1px;	
	outline:0;
	-moz-border-radius:6px 6px 0 0;	
}

ul.tabs a:hover {
	background-color:#F7F7F7;
	color:#333;
}
	
/* selected tab */
li.cur a {
	background-color:#ddd;
	border-bottom:2px double #ddd;	
	color:#000;	
	cursor:default;
}

/* tab pane */
.panes  {
	border-width:0 1px 1px 1px;
	padding:5px;
	background-color:#ddd;	
    border:2px double black;
    -moz-border-radius:4px 4px 0 0;	
}

div.panes div.gradeview{
    padding:1px;
    background-color:#ADD8E6;
}

div.content{
    padding:10px;
}

.yellow{
    background-color:#FFFF00;
}

.calendarbig table.legend,.legend tbody,.legend tbody tr, .legend tbody tr td{
    border:0 none;
    text-align:center;
}

.viewtitle{
    font-weight:bold;
}

select.multiitembox{
    height:130px;
    width:300px;
    overflow:auto;
    background-color:#ffffff;
}
