/******************* Forms ******************/
form {
    padding: 0;
    text-align: left;
}
form label {
    width: 145px;
    float: left;
    display: block;
    padding: 0;
    margin: 1.4px;
    margin-right: 10px;
}
form .checkbox label {
    display: inline;
    float:none;
}
form .radio .select label {
    display: inline;
    float:none;
}

fieldset {
    border: 1px solid #2B25F8;
    margin-top: 10px;
    margin-left: 0px;
    padding: 1em 0;
}
fieldset legend {
    color: #204b85; 
    font-size: 100%;
    font-weight: bold;
    padding-left: 5px;
    padding-right: 5px;
    margin: 0 0 0 2em;
    line-height: 150%;
}
fieldset fieldset {
    margin-top: 0px;
    margin-bottom: 20px;
    padding: 16px 0;
}
fieldset fieldset legend {
    font-size: 100%;
    font-weight: normal;
    margin-left: 20px;
    padding-left: 5px;
    padding-right: 5px;
}
fieldset fieldset div {
    clear: left;
    margin: 0 20px;
}
form div {
    clear: both;
    margin-bottom: 0em;
    padding: .5em;
    vertical-align: text-top;
}
form div.input {
    color: #444;
}
form div.datetime {
    color: #444;
}
form div.inline {
    display: inline
}
form div.required, .required {
    color: #333;
    font-weight: normal;
    background:#FFE5F7;
}
form div.submit {
    border: 0;
    clear: both;
    margin-top: 5px;
    margin-left: 140px;
}
label {
    display: inline;
    font-size: 10pt;
    padding-right: 20px;
    font-weight: bold;
}
input {
    clear: none;
    display: inline;
    font-size: 100%;
    font-family: arial, "verdana", "frutiger linotype", "lucida grande",  sans-serif;
    padding: 2px;
    border:1px solid #808080;
}
textarea {
    clear: none;
    display: inline;
    font-size: 100%;
    font-family: arial,"verdana", "frutiger linotype", "lucida grande",  sans-serif;
    padding: 2px;
    width: 60%;
    border:1px solid #808080;
}
textarea.fullview {
    clear: both;
    display: block;
    font-size: 100%;
    font-family: arial,"verdana", "frutiger linotype", "lucida grande",  sans-serif;
    padding: 2px;
    width: 100%;
    border:1px solid #808080;
    margin-left: auto;
    margin-right: auto;
}
select {
    clear: none;
    font-size: 100%;
    vertical-align: text-bottom;
    border: 1px solid #808080;
}
select[multiple=multiple] {
    width: auto;
}
option {
    font-size: 100%;
    padding: 0 3px;
    width:auto;
}

input[type=submit], input[type=button], button {
    display: inline-block;
    text-decoration: none;
    width: auto;
    height:26.5px;
    vertical-align: bottom;
    border-right: 2px outset black;
    border-bottom: 2px outset black;
    border-top: 1px outset black;
    border-left: 1px outset black;
    font-size: 12;
    padding: 3px 5px;
    color:#204b85; 
    background: url(../img/chromebg2.gif) center center repeat-x;
    margin-top:3px;
    -moz-border-radius: 5px;
	-webkit-border-radius: 5px;
}
input[type=hidden] {
    display: none;
}

input[type=checkbox] {
    clear: left;
    margin: 0px 6px 7px 2px;
    width: auto;
    border: none;
}
.radio input {
    padding: 10px; margin-left: 20px;clear: both;
}
.radio label {
    float:none; width: 80%;  display: inline;  
}

