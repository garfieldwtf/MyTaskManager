/*Menubar*/
#chromemenu{
width: auto;
font-weight: bold;
font-size: 90%;
}

#chromemenu:after{ /*Add margin between menu and rest of content in Firefox*/
content: "."; 
display: block; 
height: 0; 
clear: both; 
visibility: hidden;
}

#chromemenu ul{
border: 1px solid #BBB;
width: auto;
background: url(../img/chromebg.gif) center center repeat-x;
padding: 5px 0;
margin: 0;
text-align: right; /*set value to "right" for example to align menu to the left of page*/
}

#chromemenu ul li{
display: inline;
width: auto;
}

#chromemenu ul li a{
color: #494949;
padding: 5px;
margin: 0;
text-decoration: none;
border-right: 1px solid #DADADA;
}
#chromemenu ul li.cur a{
border: 2px solid #8C8C8C;
background: url(../img/chromebg.gif) center center repeat-x;
font-weight: bold;
}

#chromemenu ul li a:hover{
background: url(../img/chromebg2.gif) center center repeat-x;
}

.readonly{
background-color:#949197;
}

